<?xml version="1.0" encoding="utf-8"?>
<project>	
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="CheckBoxTest" package="CheckBoxTest" version="1.0.0" company="Michel de Brisis" />
	
	<!-- output -->
	<app main="Main" file="CheckBoxTest" path="bin" />
	
	<window background="#000000" fps="60" />
	<window width="800" height="480" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" version="3.2.2" />	
	<haxelib name="haxeui" version="1.7.14" />
	
	<assets path="layout" include="*" />
	
	<!-- optimize output
	<haxeflag name="-dce full" /> -->
	
</project>